    ]
  },
  "notifications": {
    "smtp": {
      "enabled": true,
      "from": "Immich Photo Server",
      "replyTo": "",
      "transport": {
        "host": "mail.your-server.de",
        "ignoreCert": true,
        "password": "${EMAIL_PASSWORD}",
